{
  "expo": {
    "name": "LiveScore",
    "description": "Live Scores app gives you live coverage for match(results, fixtures, news) for MLB and NBA and European football.",
    "slug": "liveScore",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/Icon.png",
    "splash": {
      "image": "./assets/liveScore.png",
      "backgroundColor": "#a33636",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uppercase.liveScore",
      "icon": "./assets/Icon.png",
    },
    "android": {
     "package": "com.uppercase.liveScore"
   }
  }
}
